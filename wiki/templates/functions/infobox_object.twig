<div class="infobox">
	<table>
		<tr>
			<td class="heading{% if data.beta %} beta{% endif %}" colspan="2">{{ data.name }}</td>
		</tr>
		{#<tr>
			<td colspan="2">
				<img src="/wiki/images/objects/{{ data.id }}.png" style="margin:-11px;height:100px">
			</td>
		</tr>#}
		<tr>
			<th style="width:50%">ID</th>
			<td>{{ data.id }}</td>
		</tr><tr>
			<th>Category</th>
			<td>{{ data.category }}</td>
		</tr>
		{% if data.sublayer_width %}
			<tr>
				<th>Sublayer width</th>
				<td>{{ data.sublayer_width }}
			</tr>
		{% endif %}
		{% if data.stationary %}
			<tr>
				<th>Stationary</th>
				<td>{{ data.stationary }}
			</tr>
		{% endif %}
		{% if data.version %}
			<tr>
				<th>Added in</th>
				<td>{{ data.version }}</td>
			</tr>
		{% endif %}
	</table>
</div>