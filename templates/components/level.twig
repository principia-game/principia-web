<div class="level" id="l-{{ level.id }}">
	<a class="lvlbox_top" href="/{{ page }}/{{ level.id }}">
		<img src="/{{ img }}" alt="" loading="lazy">
		{%- if featured -%}
			<span class="featured small">{{ featured }}</span>
		{%- endif -%}
		<div class="lvltitle">{{ level.title|length > 42 ? level.title|slice(0, 42) ~ '...' : level.title }}</div>
	</a>
	{{ userlink(level, 'u_') }}
</div>